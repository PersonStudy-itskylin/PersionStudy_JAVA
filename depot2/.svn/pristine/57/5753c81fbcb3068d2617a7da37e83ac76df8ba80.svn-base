public class SystemLogUtil {

	@Autowired
	private ISystemLogService syslogService;
	private SystemLog syslog;
	private Map<String, Object> json;
	/** 
	 * 方  法 名: addSysLog
	 * 功能描述： 
	 * 输入参数: SystemLogUtil
	 * 返 回 值:  SystemLogUtil
	 * 异    常：      <按照异常名字的字母顺序> 
	 * 创 建 人:  林为
	 * 创建日期: 2014年11月6日 下午2:12:49
	 * 修 改 人:  Blue_Sky
	 * 修改日期: 2014年11月6日 下午2:12:49
	 */
	public boolean addSysLog(String SystemModule,String SystemRemark,ServletActionContext servletActionContext) {
		json = syslogService.addSystemLog(syslog, servletActionContext);
		syslog.setSysname(SystemModule);
		syslog.setSysremark(SystemRemark);
		return  (Boolean) json.get("success");
	}
	
	
/*********************  getter and setter  *********************/
	public ISystemLogService getSyslogService() {
		return syslogService;
	}
	public void setSyslogService(ISystemLogService syslogService) {
		this.syslogService = syslogService;
	}
	
	
}
