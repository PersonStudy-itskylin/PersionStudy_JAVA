<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ssi.depot.dao.ISystemLogDAO">
   
   <!-- 添加数据 -->
   <insert id="insertSystemLog" parameterType="SystemLog">
	   	INSERT INTO TSYSTEMLOG(
	   		sysid,
	        accid,
	        sysname,
	        systime,
	        sysip,
	        sysremark
		)VALUES (
			#{sysid},
	        #{accid},
	        #{sysname},
	        #{systime},
	        #{sysip},
	        #{sysremark}
	    )
   </insert>
   
   <!-- 通过ID删除 -->
   <delete id="deleteSystemLog" parameterType="int">
	   	DELETE 
	   		FROM TSYSTEMLOG
		WHERE 
			sysid = #{sysid}
   </delete>
   
   <!--  通过SystemLog对象的ID 修改 -->
   <update id="updateSystemLog" parameterType="SystemLog">
   		UPDATE TSYSTEMLOG
		SET
			accid = #{accid},
			sysname = #{sysname},
			systime = #{systime},
			sysip = #{sysip},
			sysremark = #{sysremark}
		WHERE 
			sysid = #{sysid}
   </update>
   
   <!-- 全体查询 -->
   <select id="selectSystemLogByAll" resultType="SystemLog">
	   	SELECT *
	   	FROM TSYSTEMLOG
   </select>
   
   <!-- 分页查询 -->
   <select id="selectSystemLogByPage" resultType="SystemLog">
   		SELECT *
   		FROM TSYSTEMLOG
   		LIMIT #{start},#{pageSize}
   </select>
   
   <!-- 通过sysID查询 -->
   <select id="selectSystemLogById" parameterType="int" resultType="SystemLog">
   		SELECT *
   		FROM TSYSTEMLOG
   		WHERE
   			sysid = #{sysid}
   </select>
   
</mapper>